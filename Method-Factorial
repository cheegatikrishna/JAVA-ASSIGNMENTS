class factorial
{
  public static String fact(String a,String b)
  {
     Scanner sc=new Scanner(System.in);
     int fa=6;
     int n=sc.nextInt();
     if(n==fa)
     {
          return a;
         
     }
     else
     {
         return b;
         
     }
}
}
